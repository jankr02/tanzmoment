  "scripts": {
    "dev": "nx run-many --target=serve --projects=api,web",
    "serve:api": "nx serve api",
    "serve:web": "nx serve web",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:migrate": "prisma migrate dev --schema=apps/api/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=apps/api/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=apps/api/prisma/schema.prisma",
    "prisma:seed": "tsx apps/api/prisma/seed.ts",
    "db:reset": "prisma migrate reset --schema=apps/api/prisma/schema.prisma --force",
    "start": "run-s docker:up dev",
    "stop": "docker-compose down && echo 'âœ… All services stopped'"
  },
